@page "/"
@rendermode InteractiveAuto

@if (products != null)
{
    if (products.Any())
    {
        <ul>
            
            @foreach (var product in products)
            {
                <li>
                    @product.Name - <s>$ @product.Price </s> $ @product.DiscountedPrice
                </li>
            }
        </ul>
    }
    else
    {
        <div>No products</div>
    }
}
else
{
    <div>Not found</div>
}